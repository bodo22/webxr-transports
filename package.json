{
  "name": "webxr-transport",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently vite npm:devs:socket npm:devs:chii",
    "build": "vite build",
    "preview": "vite preview",
    "dev:socket": "nodemon -w socket socket/server.js",
    "devs:socket": "SECURE=true nodemon -w socket socket/server.js",
    "devs:chii": "chii start -p 5175 --https --ssl-cert .cache/cert.pem --ssl-key .cache/key.pem",
    "dev:static": "node static-server.js",
    "dev:socket:deno": "deno run --allow-net socket/deno-server.ts",
    "npx:static": "npx http-server ./public -p 8080 --cors -c-1",
    "dev:peer": "peerjs --port 9000 --key peerjs --path /myapp --verbose",
    "test": "test"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.13",
    "chii": "^1.8.0",
    "cors": "^2.8.5",
    "culori": "^3.0.1",
    "d3": "^7.8.2",
    "d3-drag": "^3.0.0",
    "express": "^4.18.2",
    "leva": "^0.9.34",
    "lodash.throttle": "^4.1.1",
    "peer": "^1.0.0",
    "peerjs": "^1.4.7",
    "playwright": "^1.31.2",
    "poline": "^0.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "send": "^0.18.0",
    "short-uuid": "^4.2.2",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "socket.io-msgpack-parser": "^3.0.2",
    "three": "^0.150.1",
    "zustand": "^4.3.6"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "concurrently": "^7.6.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tailwindcss": "^3.10.1",
    "file-system-cache": "^2.0.2",
    "ip": "^1.1.8",
    "nodemon": "^2.0.21",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7",
    "vite": "^4.2.0",
    "vite-jsconfig-paths": "^2.0.1",
    "vite-plugin-eslint": "^1.8.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "utf-8-validate": "^6.0.3"
  }
}
