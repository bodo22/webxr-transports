    "d3": "^7.8.2",
    "express": "^4.18.2",
    "peer": "^0.6.1",
    "peerjs": "^1.4.7",
    "playwright": "^1.29.1",
    "poline": "^0.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "send": "^0.18.0",
